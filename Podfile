platform :ios, ‘8.0’

target 'scholars' do
# Data
pod 'AFNetworking'
pod 'iAppInfos', '~> 0.0.6'
pod 'JSONModel' # JSONModel 解析
pod 'CocoaLumberjack' # DDLog

# UI
pod 'Masonry'        #  代码约束
pod 'WMPageController' # 类似网易新闻首页效果的实现(OC) 
pod 'EAIntroView' # 启动时页
pod 'MBProgressHUD' # 提示框
pod 'MJRefresh' # 下拉刷新
pod 'CocoaSecurity' # 用于AES加密
pod 'SDWebImage'   #  图片处理
pod 'XXNibBridge' #  视频悬浮
pod 'RongCloudIMKit'
end